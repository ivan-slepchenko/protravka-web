    "yup": "^1.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "get-version": "node -p \"require('./package.json').version\"",
    "docker-build": "npm run increase-package-version && docker build -t protravka-web .",
    "docker-run": "docker run -p 80:80 protravka-web",
    "increase-package-version": "npm version patch",
    "deploy": "npm run docker-build && wsl --setdefault Ubuntu && bash deploy.sh",
    "ssh-azure-container": "az containerapp exec --name protravka-web --resource-group protravka --container protravka-web --command \"/bin/sh\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/leaflet": "^1.9.12"
  }
